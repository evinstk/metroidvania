
#----------------------------- Global Properties ----------------------------#

/outputDir:..\bin\Debug\net471\Content
/intermediateDir:obj/$(Platform)
/platform:DesktopGL
/config:
/profile:Reach
/compress:False

#-------------------------------- References --------------------------------#

/reference:..\bin\Debug\net471\Data.dll

#---------------------------------- Content ---------------------------------#

#begin Animations/Hero.json
/copy:Animations/Hero.json

#begin Data/AnimationGroups.xml
/importer:XmlImporter
/processor:PassThroughProcessor
/build:Data/AnimationGroups.xml

#begin Data/Mobs.xml
/importer:XmlImporter
/processor:PassThroughProcessor
/build:Data/Mobs.xml

#begin Dialog/SampleDialog.json
/copy:Dialog/SampleDialog.json

#begin Fonts/DefaultFont.spritefont
/importer:FontDescriptionImporter
/processor:FontDescriptionProcessor
/processorParam:PremultiplyAlpha=True
/processorParam:TextureFormat=Compressed
/build:Fonts/DefaultFont.spritefont

#begin Maps/BossRoom.tmx
/copy:Maps/BossRoom.tmx

#begin Maps/Prison.tmx
/copy:Maps/Prison.tmx

#begin Maps/TestMap.tmx
/copy:Maps/TestMap.tmx

#begin Maps/Tiles.tsx
/copy:Maps/Tiles.tsx

#begin Maps/TransitionMap.tmx
/copy:Maps/TransitionMap.tmx

#begin Sounds/hit.wav
/importer:WavImporter
/processor:SoundEffectProcessor
/processorParam:Quality=Best
/build:Sounds/hit.wav

#begin Sounds/impact.wav
/importer:WavImporter
/processor:SoundEffectProcessor
/processorParam:Quality=Best
/build:Sounds/impact.wav

#begin Sounds/jump.wav
/importer:WavImporter
/processor:SoundEffectProcessor
/processorParam:Quality=Best
/build:Sounds/jump.wav

#begin Sounds/swing.wav
/importer:WavImporter
/processor:SoundEffectProcessor
/processorParam:Quality=Best
/build:Sounds/swing.wav

#begin Textures/hero.png
/importer:TextureImporter
/processor:TextureProcessor
/processorParam:ColorKeyColor=255,0,255,255
/processorParam:ColorKeyEnabled=True
/processorParam:GenerateMipmaps=False
/processorParam:PremultiplyAlpha=True
/processorParam:ResizeToPowerOfTwo=False
/processorParam:MakeSquare=False
/processorParam:TextureFormat=Color
/build:Textures/hero.png

#begin Textures/Tiles.PNG
/copy:Textures/Tiles.PNG

#begin Textures/Warrior.png
/importer:TextureImporter
/processor:TextureProcessor
